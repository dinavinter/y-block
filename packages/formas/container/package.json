{
    "name": "@formas/container",
    "version": "1.0.8",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@formas/tokens": "*"
    },
    "peerDependencies": {
        "atomico": "*",
        "@atomico/react": "*",
        "@atomico/vue": "*"
    },
    "devDependencies": {
        "@formas/checkbox": "latest"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library src/**/*",
        "build:exports": "exports lib/**/* types/**/* --wrappers",
        "dev:exports": "exports src/**/* --watch",
        "clean:lib": "rm -rf lib", 
        "clean:types": "rm -rf types", 
        "clean:install": "rm -rf node_modules"
    },
    "types": "types/index.d.ts",
    "exports": {
        "./element": {
            "types": "./types/element.d.ts",
            "default": "./lib/element.js"
        },
        ".": {
            "types": "./types/index.d.ts",
            "default": "./lib/index.js"
        },
        "./tsconfig.json": {
            "default": "./src/stories/tsconfig.json"
        },
        "./react": {
            "types": "./wrapper/react.d.ts",
            "default": "./wrapper/react.js"
        },
        "./preact": {
            "types": "./wrapper/preact.d.ts",
            "default": "./wrapper/preact.js"
        },
        "./vue": {
            "types": "./wrapper/vue.d.ts",
            "default": "./wrapper/vue.js"
        },
        "./README.md": {
            "default": "./types/README.md"
        }
    },
    "typesVersions": {
        "*": {
            "element": [
                "types/element.d.ts"
            ],
            "react": [
                "wrapper/react.d.ts"
            ],
            "preact": [
                "wrapper/preact.d.ts"
            ],
            "vue": [
                "wrapper/vue.d.ts"
            ]
        }
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        },
        "@atomico/vue": {
            "optional": true
        }
    }
}
