{
        "name": "@y-block/array",
        "version": "1.0.1-beta.0",
        "type": "module",
        "publishConfig": {
                "access": "public",
                "registry": "https://registry.npmjs.org/",
                "tag": "beta"
        },
        "peerDependencies": {
                "@atomico/react": "*",
                "@atomico/vue": "*",
                "atomico": "*"
        },
        "scripts": {
                "build:types": "tsc",
                "build:compile": "library \"src/**/*\"",
                "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define",
                "dev:exports": "exports src/**/* --watch"
        },
        "dependencies": {
                "@atomico/hooks": "^4.4.1",
                "yjs": "^13.6.18"
        },
        "peerDependenciesMeta": {
                "@atomico/react": {
                        "optional": true
                },
                "@atomico/vue": {
                        "optional": true
                }
        },
        "types": "types/index.d.ts",
        "exports": {
                "./element": {
                        "types": "./types/element.d.ts",
                        "default": "./lib/element.js"
                },
                ".": {
                        "types": "./types/index.d.ts",
                        "default": "./lib/index.js"
                },
                "./react": {
                        "types": "./wrapper/react.d.ts",
                        "default": "./wrapper/react.js"
                },
                "./preact": {
                        "types": "./wrapper/preact.d.ts",
                        "default": "./wrapper/preact.js"
                },
                "./vue": {
                        "types": "./wrapper/vue.d.ts",
                        "default": "./wrapper/vue.js"
                },
                "./README.md": {
                        "default": "./types/README.md"
                },
                "./tsconfig.json": {
                        "default": "./src/stories/tsconfig.json"
                }
        },
        "typesVersions": {
                "*": {
                        "element": [
                                "types/element.d.ts"
                        ],
                        "react": [
                                "wrapper/react.d.ts"
                        ],
                        "preact": [
                                "wrapper/preact.d.ts"
                        ],
                        "vue": [
                                "wrapper/vue.d.ts"
                        ]
                }
        }
}