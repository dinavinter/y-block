{
  "name": "@y-block/fragment",
  "version": "1.0.1-beta.0",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "beta"
  },
  "peerDependencies": {
    "atomico": "*",
    "@atomico/react": "*",
    "@atomico/vue": "*"
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define",

    "dev:exports": "exports src/**/* --watch"
  },
  "dependencies": {
    "yjs": "^13.6.18"
  },
  "types": "types/index.d.ts",
  "exports": {
    "./README.md": {
      "default": "./types/README.md"
    },
    "./element": {
      "types": "./types/element.d.ts",
      "default": "./lib/element.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./react": {
      "types": "./wrapper/react.d.ts",
      "default": "./wrapper/react.js"
    },
    "./preact": {
      "types": "./wrapper/preact.d.ts",
      "default": "./wrapper/preact.js"
    },
    "./vue": {
      "types": "./wrapper/vue.d.ts",
      "default": "./wrapper/vue.js"
    },
    "./tsconfig.json": {
      "default": "./src/stories/tsconfig.json"
    }
  },
  "typesVersions": {
    "*": {
      "element": [
        "types/element.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ]
    }
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  }
}