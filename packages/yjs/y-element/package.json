{
  "name": "@y-block/element",
  "version": "1.0.1-beta.2",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "peerDependencies": {
    "atomico": "*",
    "@atomico/react": "*",
    "@atomico/vue": "*"
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
    "dev:exports": "exports \"src/**/*\" --watch",
    "build": "npm run build:types && npm run build:compile && npm run build:exports",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@y-block/array": "workspace:*",
    "@y-block/fragment": "workspace:*"
  },
  "dependencies": {
    "yjs": "^13.6.18"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  },
  "exports": {
    "./element": {
      "types": "./src/element.tsx",
      "default": "./src/element.tsx"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./tsconfig.json": {
      "default": "./src/stories/tsconfig.json"
    },
    "./element2": {
      "default": "./lib/element2.js"
    },
    "./react": {
      "types": "./wrapper/react.d.ts",
      "default": "./wrapper/react.js"
    },
    "./preact": {
      "types": "./wrapper/preact.d.ts",
      "default": "./wrapper/preact.js"
    },
    "./vue": {
      "types": "./wrapper/vue.d.ts",
      "default": "./wrapper/vue.js"
    },
    "./README.md": {
      "default": "./types/README.md"
    }
  },
  "typesVersions": {
    "*": {
      "element": [
        "types/element.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ]
    }
  }
}