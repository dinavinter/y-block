{
  "name": "@y-block/gallery",
  "version": "1.0.1-beta.2",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "beta"
  },
  "peerDependencies": {
    "atomico": "*",
    "@atomico/react": "*",
    "@atomico/vue": "*"
  },
  "dependencies": {
    "@y-block/array": "workspace:*",
    "@y-block/map": "workspace:*",
    "yjs": "^13.6.18"
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
    "dev:exports": "exports src/**/* --watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.4",
    "@atomico/postcss-tokens": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.27",
    "@storybook/types": "8.1.11",
    "@tailwindcss/aspect-ratio": "^0.4.2"
  },
  "postcss": {
    "plugins": {
      "@atomico/postcss-tokens": {},
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "exports": {
    "./tsconfig.json": {
      "default": "./src/stories/tsconfig.json"
    },
    "./element": {
      "types": "./types/element.d.ts",
      "default": "./lib/element.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./react": {
      "types": "./wrapper/react.d.ts",
      "default": "./wrapper/react.js"
    },
    "./preact": {
      "types": "./wrapper/preact.d.ts",
      "default": "./wrapper/preact.js"
    },
    "./vue": {
      "types": "./wrapper/vue.d.ts",
      "default": "./wrapper/vue.js"
    },
    "./README.md": {
      "default": "./types/README.md"
    }
  },
  "typesVersions": {
    "*": {
      "element": [
        "types/element.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ]
    }
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  }
}