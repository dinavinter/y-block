{
        "name": "@y-block/map",
        "version": "1.0.1-beta.2",
        "type": "module",
        "publishConfig": {
                "access": "public"
        },
        "peerDependencies": {
                "atomico": "*",
                "@atomico/react": "*",
                "@atomico/vue": "*"
        },
        "devDependencies": {
           "@y-block/array": "workspace:*"
        },
        "scripts": {
                "build:types": "tsc",
                "build:compile": "library \"src/**/*\"",
                "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --main define",
                "dev:exports": "exports src/**/* --watch"
        },
        "dependencies": {
                "yjs": "^13.6.18"
        },
        "peerDependenciesMeta": {
                "@atomico/react": {
                        "optional": true
                },
                "@atomico/vue": {
                        "optional": true
                }
        },
        "exports": {
                "./element": {
                        "types": "./types/element.d.ts",
                        "default": "./lib/element.js"
                },
                ".": {
                        "types": "./types/index.d.ts",
                        "default": "./lib/index.js"
                },
                "./react": {
                        "types": "./wrapper/react.d.ts",
                        "default": "./wrapper/react.js"
                },
                "./preact": {
                        "types": "./wrapper/preact.d.ts",
                        "default": "./wrapper/preact.js"
                },
                "./vue": {
                        "types": "./wrapper/vue.d.ts",
                        "default": "./wrapper/vue.js"
                },
                "./README.md": {
                        "default": "./types/README.md"
                }
        },
        "typesVersions": {
                "*": {
                        "element": [
                                "types/element.d.ts"
                        ],
                        "react": [
                                "wrapper/react.d.ts"
                        ],
                        "preact": [
                                "wrapper/preact.d.ts"
                        ],
                        "vue": [
                                "wrapper/vue.d.ts"
                        ]
                }
        }
}