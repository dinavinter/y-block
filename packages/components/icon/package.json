{
    "name": "@formas/icon",
    "version": "1.0.8",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@formas/tokens": "*"
    },
    "peerDependencies": {
        "atomico": "*",
        "@atomico/react": "*",
        "@atomico/vue": "*"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library \"src/**/*\" --all-external",
        "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
        "dev:exports": "exports \"src/**/*\" --watch"
    },
    "types": "types/index.d.ts",
    "exports": {
        "./icon": {
            "types": "./src/icon.tsx",
            "default": "./src/icon.tsx"
        },
        "./icons": {
            "types": "./types/icons.d.ts",
            "default": "./lib/icons.js"
        },
        ".": {
            "types": "./types/index.d.ts",
            "default": "./lib/index.js"
        },
        "./stories/index.stories": "./src/stories/index.stories.tsx",
        "./tsconfig.json": {
            "default": "./src/stories/tsconfig.json"
        },
        "./icon2": {
            "default": "./lib/icon2.js"
        },
        "./react": {
            "types": "./wrapper/react.d.ts",
            "default": "./wrapper/react.js"
        },
        "./preact": {
            "types": "./wrapper/preact.d.ts",
            "default": "./wrapper/preact.js"
        },
        "./vue": {
            "types": "./wrapper/vue.d.ts",
            "default": "./wrapper/vue.js"
        },
        "./README.md": {
            "default": "./types/README.md"
        }
    },
    "typesVersions": {
        "*": {
            "icon": [
                "types/icon.d.ts"
            ],
            "icons": [
                "types/icons.d.ts"
            ],
            "stories/index.stories": [
                "src/stories/index.stories.tsx"
            ],
            "react": [
                "wrapper/react.d.ts"
            ],
            "preact": [
                "wrapper/preact.d.ts"
            ],
            "vue": [
                "wrapper/vue.d.ts"
            ]
        }
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        },
        "@atomico/vue": {
            "optional": true
        }
    }
}
