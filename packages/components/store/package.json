{
  "name": "@y-block/store",
  "version": "1.0.2-beta.14",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "beta"
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library src/*",
    "build:exports": "exports lib/* types/* --wrappers",
    "dev:exports": "exports src/* --watch",
    "build": "pnpm  build:types && pnpm build:compile && pnpm build:exports",
    "build:publish": "pnpm publish  --tag beta --no-git-check ",
    "dev": "vite dev",
    "start": "vite",
    "clean:lib": "rm -rf lib",
    "clean:types": "rm -rf types wrapper",
    "clean:install": "rm -rf node_modules"
  },
  "peerDependencies": {
    "@atomico/react": "*",
    "@atomico/vue": "*",
    "atomico": "*"
  },
  "devDependencies": {
    "@atomico/tsconfig": "^1.0.0",
    "@atomico/vite": "^2.21.1",
    "@atomico/exports": "latest",
    "@atomico/vue": "latest",
    "@atomico/store": "^2.0.1",

    "@hocuspocus/provider": "^2.13.6",
    "y-protocols": "^1.0.6",
    "y-websocket": "^2.0.4"

  },
  "dependencies": {
    "yjs": "latest"
  },
  "overrides": {
    "yjs": "13.6.19"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  },
  "exports": {
    "./doc.define": {
      "types": "./types/doc.define.d.ts",
      "default": "./lib/doc.define.js"
    },
    "./doc": {
      "types": "./src/doc.tsx",
      "default": "./src/doc.tsx"
    },
    "./hocuspocus": {
      "types": "./types/hocuspocus.d.ts",
      "default": "./lib/hocuspocus.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./ws": {
      "types": "./types/ws.d.ts",
      "default": "./lib/ws.js"
    },
    "./doc.readme.md": {
      "default": "./src/doc.readme.md"
    }
  },
  "typesVersions": {
    "*": {
      "element": [
        "types/element.d.ts"
      ],
      "react": [
        "wrapper/react.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ],
      "index 2/react": [
        "wrapper/index 2/react.d.ts"
      ],
      "index 2/preact": [
        "wrapper/index 2/preact.d.ts"
      ],
      "index 2/vue": [
        "wrapper/index 2/vue.d.ts"
      ],
      "index 3/react": [
        "wrapper/index 3/react.d.ts"
      ],
      "index 3/preact": [
        "wrapper/index 3/preact.d.ts"
      ],
      "index 3/vue": [
        "wrapper/index 3/vue.d.ts"
      ],
      "index 4/react": [
        "wrapper/index 4/react.d.ts"
      ],
      "index 4/preact": [
        "wrapper/index 4/preact.d.ts"
      ],
      "index 4/vue": [
        "wrapper/index 4/vue.d.ts"
      ],
      "array": [
        "src/array.tsx"
      ],
      "map": [
        "src/map.tsx"
      ],
      "props": [
        "src/props.tsx"
      ],
      "fragment": [
        "types/fragment.d.ts"
      ],
      "element/react": [
        "wrapper/element/react.d.ts"
      ],
      "element/preact": [
        "wrapper/element/preact.d.ts"
      ],
      "element/vue": [
        "wrapper/element/vue.d.ts"
      ],
      "fragment/react": [
        "wrapper/fragment/react.d.ts"
      ],
      "fragment/preact": [
        "wrapper/fragment/preact.d.ts"
      ],
      "fragment/vue": [
        "wrapper/fragment/vue.d.ts"
      ],
      "element.stories": [
        "types/element.stories.d.ts"
      ],
      "fragment.stories": [
        "types/fragment.stories.d.ts"
      ],
      "doc": [
        "types/doc.d.ts"
      ],
      "doc.define": [
        "types/doc.define.d.ts"
      ],
      "doc.stories": [
        "types/doc.stories.d.ts"
      ],
      "ws": [
        "types/ws.d.ts"
      ],
      "hocuspocus": [
        "types/hocuspocus.d.ts"
      ]
    }
  }
}