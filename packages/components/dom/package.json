{
  "name": "@y-block/dom",
  "version": "1.0.2-beta.14",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "beta"
  },
  "peerDependencies": {
    "@atomico/react": "*",
    "@atomico/vue": "*",
    "atomico": "*"
  },
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library src/*",
    "build:exports": "exports lib/* types/* --wrappers",
    "dev:exports": "exports src/* --watch",
    "deploy": "pnpm build && pnpm publish --tag beta --no-git-check",
    "build": "pnpm  build:types && pnpm build:compile && pnpm build:exports",
    "build:publish": "pnpm publish  --tag beta --no-git-check ",
    "prepublishOnly": "pnpm  build",
    "start": "vite",
    "clean:lib": "rm -rf lib",
    "clean:types": "rm -rf types wrapper",
    "clean:install": "rm -rf node_modules"
  },
  "dependencies": {
    "@atomico/store": "^2.0.1",
    "@twind/aspect-ratio": "^0.1.4",
    "@twind/content": "^0.1.0",
