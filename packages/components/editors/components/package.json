{
    "name": "@formas/components",
    "version": "1.0.8",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev:exports": "pnpm run centralize",
        "build:centralize": "exports \"../**\" --centralize-packages --wrappers",
        "clean:lib": "rm -rf lib",
        "clean:type": "rm -rf types wrapper",
        "clean:install": "rm -rf node_modules"
    },
    "peerDependencies": {
        "@atomico/hooks": "^4.1.1",
        "atomico": "^1.77.1",
        "@atomico/react": "*",
        "@atomico/vue": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        },
        "@atomico/vue": {
            "optional": true
        }
    },
    "main": "wrapper/index.js",
    "module": "wrapper/index.js",
    "types": "wrapper/index.d.ts",
    "dependencies": {
        "@y-block/cm": "^1.0.2-beta.15"
    },
    "exports": {
        ".": {
            "types": "./wrapper/index.d.ts",
            "default": "./wrapper/index.js"
        },
        "./@y-block/cm": {
            "types": "./wrapper/@y-block/cm.d.ts",
            "default": "./wrapper/@y-block/cm.js"
        },
        "./react": {
            "types": "./wrapper/react.d.ts",
            "default": "./wrapper/react.js"
        },
        "./preact": {
            "types": "./wrapper/preact.d.ts",
            "default": "./wrapper/preact.js"
        },
        "./vue": {
            "types": "./wrapper/vue.d.ts",
            "default": "./wrapper/vue.js"
        },
        "./package.json": {
            "default": "../codemirror/package.json"
        },
        "./package": {
            "default": "../components/package.json"
        }
    },
    "typesVersions": {
        "*": {
            "@y-block/cm": [
                "wrapper/@y-block/cm.d.ts"
            ],
            "react": [
                "wrapper/react.d.ts"
            ],
            "preact": [
                "wrapper/preact.d.ts"
            ],
            "vue": [
                "wrapper/vue.d.ts"
            ]
        }
    }
}
